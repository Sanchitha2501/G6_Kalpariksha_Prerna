#include <stdio.h>
#include <string.h>

int main() 
{
    char string[100];
    printf("Enter a string: ");
    scanf("%[^\n]s", string);
    int length = strlen(string);
    char repeat[100];
    int repeat_count = 0;
    for (int i = 0; i < length; i++)
    {
    char current = string[i];
    int count = 0;
    for (int j = 0; j < length; j++)
    {
       if (string[j] == current) 
       {
           count++;
        }
    }
    if (count > 1 && strchr(repeat, current) == NULL)
        {
            repeat[repeat_count] = current;
            repeat_count++;
        }
    }
    printf("Repeated output: ");
    for (int i = 0; i < repeat_count; i++) 
    {
        printf("%c ", repeat[i]);
    }
    printf("\n");
    
    return 0;
}
