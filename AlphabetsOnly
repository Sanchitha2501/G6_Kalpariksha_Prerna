
#include <stdio.h>

int main()
{
    char string[100];
    int i,j;
    scanf("%[^\n]s", string);
    //printf("%s",string);
    for(i=0; string[i]!='\0'; ++i)
    {
        while (!((string[i]>='A'&&string[i]<='Z') || (string[i]>='a'&&string[i]<='z' || string[i]=='\0')))
        {
            for(j=i;string[j]!='\0';++j)
            {
                string[j]=string[j+1];
            }
            string[j]='\0';
        }
    }
    printf("Output: %s",string);

    return 0;
}
